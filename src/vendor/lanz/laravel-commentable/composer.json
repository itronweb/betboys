    "keywords": ["laravel", "commentable", "comment", "threaded", "nested set", "laravel 4", "laravel 5"],
    "license": "MIT",
    "authors": [
        {
            "name": "Romain Lanz",
            "email": "lanz.romain@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "illuminate/support": "~5.0",
        "illuminate/database": "~5.0",
        "baum/baum": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "classmap": [
